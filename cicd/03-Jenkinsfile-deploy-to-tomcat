pipeline {
  agent any
  tools {
    maven 'maven' 
  }
  stages {
    stage ('Build') {
      steps {
        bat 'mvn clean package'
	
      }
    }
	
    
    stage ('Deploy') {
      steps {
        script {
          deploy adapters: [tomcat9(credentialsId: 'tomcat_credential', path: '', url: 'http://localhost:8080')], contextPath: '/push', onFailure: false, war: 'target/*.war' 
        }
      }
    
	}
	}
	}
