pipeline {
  agent any
  tools {
    maven 'maven' 
  }
  
  stages {
    stage ('Build') {
      steps {
        sh 'mvn clean package'
      }
    }
    stage ('Deploy') {
      steps {
        script {
          deploy adapters: [tomcat9(credentialsId: 'tomcat_credential', path: '', url: 'http://52.66.88.59:8090')], contextPath: '/pushparaj', onFailure: false, war: target/helloworld.war' 
        }
      }
    }
  }
  }
