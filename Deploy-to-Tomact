pipeline {
  agent any
  tools {
    maven 'maven-3.6.3' 
  }
  stages {
    stage ('Build') {
      steps {
        sh 'mvn clean package'
      }
    }
    stage ('Deploy') {
      steps {
        script {
          deploy adapters: [tomcat9(credentialsId: 'tomcat_credential', path: '', url: 'http://52.66.88.59:8090')], contextPath: '/pushparaj', onFailure: false, war: 'webapp/target/*.war' 
        }
      }
    }
  }
